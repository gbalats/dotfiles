# .bash_profile

export EDITOR='emacs'
export TERM=xterm-256color

# Get the aliases and functions
if [ -f ~/.bashrc ]; then
    . ~/.bashrc
fi

# User specific environment and startup programs
pathmunge $HOME/bin
pathmunge $HOME/.local/bin
pathmunge $HOME/.cask/bin

export PATH

